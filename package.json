{
  "name": "github-conventional-commit-enforcer",
  "version": "1.0.0",
  "description": "GitHub Action to enforce conventional commits on PR merges",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "ncc build src/index.js -o dist",
    "prepare": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "eslint src/ __tests__/",
    "lint:fix": "eslint src/ __tests__/ --fix"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^6.0.0"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@eslint/js": "^8.0.0"
  },
  "keywords": [
    "github-actions",
    "conventional-commits",
    "ci",
    "git"
  ],
  "author": "",
  "license": "MIT"
}
